NODE_ENV=development

# Feature flags
USE_DATABASE=true

# Minecraft Server
MINECRAFT_HOST=localhost
MINECRAFT_PORT=25565
MINECRAFT_VERSION=1.21.10

# Bot Credentials (offline mode)
BOT_USERNAME_1=TestBot1
BOT_USERNAME_2=TestBot2
BOT_USERNAME_3=TestBot3

# Optional: Client URL
CLIENT_URL=http://localhost:5173

# Supabase PostgreSQL
# DATABASE_URL uses PgBouncer pooler (port 6543) — for runtime connections
# DIRECT_URL uses direct connection (port 5432) — for Prisma CLI (migrations, db push)
# Note: special characters in password must be URL-encoded (+ → %2B, % → %25)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://USER:PASSWORD@HOST:5432/postgres"

# Discord
DISCORD_BOT_TOKEN=your_bot_token
DISCORD_GUILD_ID=your_guild_id

# ElevenLabs (voice TTS)
ELEVENLABS_API_KEY=your_api_key

# LLM Providers (OpenRouter)
OPENROUTER_API_KEY=your_api_key
